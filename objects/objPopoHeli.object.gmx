        if (global.sectionRight &gt; view_xview[0] + view_wview[0] &amp;&amp; global.sectionLeft != view_xview)
            x = view_xview[0] - 4;
        else
            x = max(prevX - 96, view_xview[0] - 4);
    }
    
    lenght = abs(x - prevX);
    
    swooceIn = true;
    shootTimer = 12;
    moveTimer = 150;
    playedSound = false;
    xspeed = 0.25 * image_xscale;
    yspeed = 0;
}
else
{
    // prevXView=view_xview[0]+image_xscale;
    stopSFX(sfxPopoHeliFire);
}
