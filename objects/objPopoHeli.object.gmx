prevY = ystart;
shootTimer = 12;
moveTimer = 150; // How long is the flamethrower active?
playedSound = false;
accel = 0.08;
spawnHeightOffset = -48;

prevXView = view_xview;
lenght = 112;
init = 0;
</string>
