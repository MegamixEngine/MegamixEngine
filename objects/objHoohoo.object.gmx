    if (animTimer == 22 &amp;&amp; image_index == 2)
    {
        image_index = 3;
        animTimer = 0;
    }
    
    // Wave movement
    sinCounter += .15;
    yspeed = -((sin(sinCounter) / (hasRock + 1)) * 1.7);
    
    // release Dwayne
    if (collision_rectangle(x - 40, y - 512, x + 40, y + 512, target, false, true) &amp;&amp; hasRock)
