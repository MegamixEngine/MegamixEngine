if (entityCanStep())
{
    // Animation
    animTimer++;
    
    // wing flap
    if (animTimer == 10 &amp;&amp; image_index != 2)
    {
        if (image_index == 0 || image_index == 1)
        {
            image_index = !image_index;
        }
        
