/// Enemy AI

if (entityCanStep())
{
    var prevCanShoot = canShoot;
    if (instance_exists(target))
    {
        if (abs(x - target.bbox_left + (abs(target.bbox_left - target.bbox_right) / 2)) &lt; 16 * 4)
        {
            canShoot = true;
        }
        else
            canShoot = false;
        if (instance_exists(bullets[0]) || instance_exists(bullets[1]))
            canShoot = false;
    }
    else
        canShoot = false;
    
    if (((sign(prevYspeed) == -1 &amp;&amp; yspeed &gt;= 0 &amp;&amp; sign(image_yscale == 1))
        || (sign(prevYspeed) == 1 &amp;&amp; yspeed &lt;= 0 &amp;&amp; sign(image_yscale == -1)))
        &amp;&amp; floor(animFrame != 0))
    {
        event_user(0);
        animFrame = 3;
    }
    
    if (ground &amp;&amp; yspeed == 0 &amp;&amp; floor(animFrame) != 1)
        animFrame = 0;
    else if (ground &amp;&amp; yspeed == 0 &amp;&amp; floor(animFrame &gt;= 3))
        attackTimer = 0;
    
    if (!ground)
        canShoot = false;
    if (!prevCanShoot &amp;&amp; canShoot &amp;&amp; animFrame == 0)
        attackTimer = attackTime + 1;
    
    if (attackTimer &gt; attackTime)
    {
        if (ground &amp;&amp; animFrame == 0)
        {
            animFrame = 1;
        }
        else if (ground &amp;&amp; floor(animFrame == 2))
        {
            yspeed = jumpSpeed * image_yscale;
            attackTimer = 0;
        }
    }
    
    if (ycoll != 0 &amp;&amp; ground)
        playSFX(sfxHeavyLand);
    
    if (floor(animFrame) == 1 || floor(animFrame) &gt;= 3)
    {
        animFrame += animSpeed;
        if (floor(animFrame) &gt; 5)
            animFrame = 5;
    }
    image_index = animFrame;
    prevYspeed = yspeed;
}
