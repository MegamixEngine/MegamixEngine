                        {
                            // end boost stuff
                            boost = false;
                            cooldown = 60; // &lt;-- cooldown wait time here
                            imgIndex = 0;
                        }
                    }
            }
        }
        
        if (cooldown != 0)
        {
            cooldown -= 1;
        }
        
        if (boost)
        {
            xspeed = boostSpd * image_xscale;
        }
        else
        {
            xspeed = normalSpd * image_xscale;
        }
        
        // animation
        blinkTimer += 1;
        if (blinkTimer &gt;= 12)
        {
            eyeLight = !eyeLight;
            blinkTimer = 0;
        }
        
        if (boost)
        {
            imgIndex += imgSpd;
            if (imgIndex &gt;= 5)
            {
                imgIndex = 3 + imgIndex mod 5;
            }
        }
    }
    else
    {
        xspeed = 0;
        boost = false;
        cooldown = 0;
        imgIndex = 0;
        alarmStop -= 1;
        if (!alarmStop)
        {
            stopped = false;
            xspeed = normalSpd * image_xscale;
        }
    }
