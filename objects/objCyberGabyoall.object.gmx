            }
            else
            {
                cooldown = 0;
            }
        }
        
        if (instance_exists(target))
        {
            switch (phase)
            {
                // Behavior
                case 1:
                    if (cooldown == 0)
                    {
                        if (!boost &amp;&amp; collision_line(x - 512,
                            y + sprite_height - image_yscale, x + 512,
                            y + sprite_height, target, false, true))
                        {
                            boost = true;
