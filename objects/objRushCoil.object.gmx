        with (prtEntity)
        {
            if (dead)
            {
                continue;
            }
            
            if (global.factionStance[other.faction, faction])
            {
                continue;
            }
            
            if (grav == 0)
            {
                continue;
            }
            
            if ((yspeed * other.image_yscale &gt; 0))
            {
                if (blockCollision)
                {
                    if (faction != 0)
                    {
                        if (place_meeting(x, y, other))
                        {
                            if (!place_meeting(x, y - yspeed, other))
                            {
                                with (other)
