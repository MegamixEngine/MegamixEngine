                                    other.climbing = 0;
                                    var coilPower = -7.5;
                                    if (checkCheats(cheatEnums.wiiPhysics))
                                    {
                                        coilPower = -8;
                                    }
                                    other.yspeed = coilPower * image_yscale;
                                    
                                    sprite_index = sprRushCoil;
                                    timer = 60;
                                }
                            }
                        }
                    }
                }
            }
        }
    }
    
    if (!ground)
    {
        timer = 1;
    }
    
    if (!ammoCanAfford(playerID))
    {
        timer = 1;
    }
    
    if (timer)
    {
        timer--;
        
        if (!timer)
        {
            if (global.characterSelected[playerID] == CHAR_PROTOMAN) // Alternate destroy effect for Proto Mode
            {
                instance_create(x, bboxGetYCenter(), objExplosion);
