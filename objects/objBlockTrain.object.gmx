                    i.origin = id;
                }
                i.xx = xx;
                i.yy = yy;
                i.imgdelay = imgdelay;
                i.spawndelay = spawndelay;
                i.trainlength = trainlength;
                i.sprite_index = sprite_index;
                
                with (i)
                {
                    if (!insideSection(bboxGetXCenter(), bboxGetYCenter()))
                    {
                        instance_destroy();
                        visible = 0;
                    }
                    else if (place_meeting(x, y, objMegaman)) // Push Mega out
                    {
                        ys = sign(yy);
                        if (ys == 0)
                        {
