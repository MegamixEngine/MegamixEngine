                        }
                        with (objMegaman)
                        {
                            while (place_meeting(x, y, other.id))
                            {
                                y += other.ys;
                            }
                        }
                    }
                }
            }
            break;
        case 4: // Build off 
            if (actionTimer == (spawndelay + imgdelay * 2)
