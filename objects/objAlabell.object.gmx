            dead = true;
            explosion = instance_create(x, spriteGetYCenter(), objHarmfulExplosion);
            explosion.contactDamage = 8;
            explosion.damageEnemies = true;
            playSFX(sfxMM9Explosion);
        }
    }
}
else if (dead)
