            break;
        case 9:
            sprite_index = sprAlabell9;
            break;
        default:
            break;
    }
    
    if (actionTimer == 0)
    {
        if (currentTime != 1)
        {
            actionTimer = 60;
            currentTime -= 1;
            playSFX(sfxCountBomb);
            exit;
        }
        else
