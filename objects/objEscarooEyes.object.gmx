            dir = 0;
    }
    
    if (dir == 0 &amp;&amp; shootTimer == 110)
        dir = -1;
    
    // dir. hacky as FART
    x -= xspeed;
    y -= yspeed;
    
    x += xspeed * dir;
    y += yspeed * dir;
    
    if (shootTimer != 90 &amp;&amp; dir == -1 &amp;&amp; sign(getX - x) == sign(xspeed) &amp;&amp; sign(getY - y) == sign(yspeed))
    {
        image_index = 0;
        xspeed = 0;
        yspeed = 0;
        x = getX;
        y = getY;
        dir = 1;
    }
}
else if (!insideView())
{
