        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (sfx != noone)
{
    play = insideSection(x + 8, y + 8) &amp;&amp; insideView();
    
    // positive interval: play periodically
    if (!global.frozen &amp;&amp; interval &gt; 0)
    {
        timer++;
        if (timer &gt;= interval &amp;&amp; play)
        {
            playSFX(sfx,volume);
            timer = 0;
        }
        if (!play)
        {
            timer = 0;
        }
    }
    
    // 0 interval: play once
    if (!global.frozen &amp;&amp; interval == 0)
    {
        if (!play)
        {
            timer = -1;
        }
        else if (timer == 0)
        {
            playSFX(sfx,volume);
            timer = -1;
        }
    }
    
    // -1 interval: loop
    if (interval == -1)
    {
        play &amp;= !global.frozen;
        if (play)
        {
            if (!audio_is_playing(sfx))
            {
                loopSFX(sfx,volume);
            }
        }
        else
        {
            // don't stop if another emitter is playing it
            var skipStop = false;
            with (object_index)
            {
                if (play &amp;&amp; id != other.id &amp;&amp; sfx == other.sfx)
                    skipStop = true;
            }
            
            // stop sfx
            if (!skipStop)
            {
                stopSFX(sfx);
            }
        }
    }
}
</string>
