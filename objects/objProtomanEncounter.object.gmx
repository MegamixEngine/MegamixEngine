        visible = true;
        y = view_yview[0] + 8;
    }
    if (phase == 2)
    {
        y -= 8;
        if (y &lt; view_yview[0] - 16)
            instance_destroy();
    }
    else if (y &lt; ystart) // descend
        y += 8;
    else
    {
        y = ystart;
        if (phase == 0)
            animTimer++;
        else
            animTimer--;
        image_index = 3 - (animTimer div 4 mod 2);
        if (animTimer &gt; 16)
        {
            image_index = 0;
            if (animTimer &gt; 100 + 100 * (itemDrop != -1))
            {
                animTimer = 15;
                phase = 1;
            }
            if (itemDrop != -1 &amp;&amp; animTimer == 150)
            {
                with (instance_create(x, y, itemDrop))
                {
                    xspeed = other.image_xscale * 2;
                    yspeed = -3;
                }
            }
        }
        if (animTimer &lt; 0)
        {
            image_index = 1;
            phase = 2;
            with (objBossBarrier)
            {
                delay = 0;
                event_user(0);
            }
            resumeMusic();
            playerFreeMovement(protoLock);
        }
    }
}
