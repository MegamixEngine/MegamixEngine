    if (shootTimer == 70 + 20)
    {
        var ball = instance_create(x + 32 * image_xscale, y + 8, objTamaYarnBall);
        ball.xspeed = image_xscale;
    }
    if (shootTimer == 70 + 25)
        image_index = 1;
    if (shootTimer == 70 + 30)
        image_index = 0;
    
    if (shootTimer == 110 &amp;&amp; instance_exists(objTamaYarnBall))
        shootTimer -= 10;
    
    if (shootTimer == 120)
        image_index = 3;
    if (shootTimer == 125)
        image_index = 4;
    
    if (shootTimer == 130)
    {
        for (var i = 0; i &lt;= 2; i += 1)
        {
            var flea = instance_create(x - 8 * image_xscale, y, objTamaFlea);
            flea.xspeed = image_xscale + (sign(image_xscale)*((i+5) / 5));
            flea.yspeed = -(3 + i);
        }
    }
    if (shootTimer == 140)
        image_index = 3;
    if (shootTimer == 145)
    {
        if (instance_exists(objTamaFlea))
            shootTimer -= 4;
        image_index = 0;
    }
    
    shootTimer += 1;
    if (shootTimer &gt;= endTimer)
        shootTimer = 0;
    
    tailWag += 1;
    if (tailWag == floor(tailWag / 10) * 10)
    {
        if (tailIndex == 0)
            tailIndex = 1;
        else
            tailIndex = 0;
    }
}
else if (!insideView())
{
    image_index = 0;
    shootTimer = 0;
}
