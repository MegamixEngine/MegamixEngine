            }
            
            if (shootTimer &gt;= 60 + (8 * 5))
            {
                shootTimer = 0;
                attack = choose(0, 1);
            }
        }
    }
    
    if ((x &gt; view_xview + 64 &amp;&amp; image_xscale == -1)
