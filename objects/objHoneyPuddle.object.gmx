if (!dead &amp;&amp; !global.frozen &amp;&amp; !global.timeStopped)
{
    if (!instance_exists(caughtPlayer))
    {
        with (objMegaman)
        {
            with (other)
            {
                if (place_meeting(x, y, other) &amp;&amp; other.ground)
                {
                    if (!other.honeyLock)
                    {
                        // undo any active player locks
                        with (other)
                        {
                            climbing = false;
                            isSlide = false;
                            climbLock = lockPoolRelease(climbLock);
                            slideLock = lockPoolRelease(slideLock);
                            slideChargeLock = lockPoolRelease(slideChargeLock);
                            mask_index = mskMegaman;
                            xspeed = 0;
                            
                            with (objRushCycle)
                            {
                                if (playerID == other.playerID)
                                {
                                    // Teleport away
                                    i = instance_create(x, y, objRushTeleport);
                                    i.upordown = 'up';
                                    i.parent = parent;
                                    i.image_yscale = image_yscale;
                                    
                                    instance_destroy();
                                }
                            }
                            
                            with (objBreakDash)
                            {
                                if (playerID == other.playerID)
                                    instance_destroy();
                            }
                        }
                        
                        // honey lock
                        with (other)
                        {
                            honeyLock = lockPoolLock(
                                localPlayerLock[PL_LOCK_MOVE],
                                localPlayerLock[PL_LOCK_JUMP],
                                localPlayerLock[PL_LOCK_GRAVITY],
                                localPlayerLock[PL_LOCK_SHOOT],
                                localPlayerLock[PL_LOCK_CHARGE],
                                localPlayerLock[PL_LOCK_SLIDE],
                                localPlayerLock[PL_LOCK_CLIMB]);
                            
                            shiftObject(other.x - x, 0, true);
                        }
                        
                        caughtPlayer = other.id;
                        caughtXScalePrev = other.image_xscale;
                    }
                }
            }
        }
    }
    else if (caughtTimer &gt;= 8 + ((global.difficulty == DIFF_HARD) * 2))
    {
        // release player
        with (caughtPlayer)
        {
            if (honeyLock)
            {
                honeyLock = lockPoolRelease(honeyLock);
            }
        }
        
        instance_create(x, y, objExplosion);
        instance_destroy();
        exit;
    }
    
    // despawn timer
    despawnTimer += 1;
    if (despawnTimer &gt;= 370)
    {
        with (caughtPlayer)
        {
            if (honeyLock)
            {
                honeyLock = lockPoolRelease(honeyLock);
            }
        }
        
        instance_destroy();
        exit;
    }
    else if (despawnTimer &gt;= 300)
    {
        if (despawnTimer mod 2)
        {
            visible = !visible;
        }
    }
}
