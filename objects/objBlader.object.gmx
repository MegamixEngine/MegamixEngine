        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Set color on spawn
if (init)
{
    init = 0;
    if (col != -1)
    {
        switch (col)
        {
            case 0:
                sprite_index = sprBlader;
                break;
            case 1:
                sprite_index = sprBladerBlue;
                break;
        }
    }
}

event_inherited();

if (entityCanStep())
{
    image_speed = imgSpd;
    attackTimer++;
    switch (phase)
    {
        case 1: // Look for target 
            xspeed = moveSpeed * image_xscale;
            if (blockCollision)
            {
                xSpeedTurnaround();
            }
            if (target &amp;&amp; attackTimer &gt;= attackTimerMax)
            {
                if (abs(target.x - x) &lt; radius &amp;&amp; yspeed == 0 &amp;&amp; round(target.y) != round(y))
                {
                    if (xspeedDive)
                        xspeed = (target.x - x) / diveMult;
                    else
                        xspeed = moveSpeed;

                    yspeed = (target.y - y) / diveMult;

                    targetY = target.y;
                    phase = 2;
                    calibrateDirection();
                    
                    if (shotSound[0] != noone)
                    {
                        playSFX(shotSound[0]);
                    }
                    
                    if (shotObject[0] != noone)
                    {
                        var inst = instance_create(spriteGetXCenter(),spriteGetYCenter(),shotObject[0]);
                        if (shotSprite[0] != noone)
                        {
                            inst.sprite_index = shotSprite[0];
                            inst.mask_index = inst.sprite_index;
                        }
                        inst.image_xscale = image_xscale;
                        inst.image_yscale = image_yscale;
                        inst.dir = sign(image_xscale);
                        
                    }
                }
            }
            break;
        case 2: // Swoop at target 
            yspeed += 0.025 * sign(yspeed);
            xspeed += 0.025 * sign(xspeed);
            
            if ((sign(y - targetY) == sign(yspeed)) || (blockCollision &amp;&amp; ycoll != 0))
            {
                phase = 3;
                if (blockCollision)
                    yspeed = -max(yspeed,1);
                else
                    yspeed = -yspeed;
            }
            break;
        case 3: // Move back to original height 
            yspeed -= 0.025 * sign(yspeed);
            xspeed -= 0.025 * sign(xspeed);
            if (blockCollision)
            {
                xSpeedTurnaround();
            }
            if (sign(y - ystart) == sign(yspeed))
            {
                attackTimer = 0;
                phase = 1;
                xspeed = 0;
                yspeed = 0;
                shiftObject(0, ystart - y, blockCollision);
                //cooldown = cooldownMax;
                calibrateDirection();
            
                if (shotSound[1] != noone)
                {
                    playSFX(shotSound[1]);
                }
                
                if (shotObject[1] != noone)
                {
                    var inst = instance_create(spriteGetXCenter(),spriteGetYCenter(),shotObject[1]);
                    if (shotSprite[1] != noone)
                    {
                        inst.sprite_index = shotSprite[1];
                        inst.mask_index = inst.sprite_index;
                    }
                    inst.image_xscale = image_xscale;
                    inst.image_yscale = image_yscale;
                    inst.dir = sign(image_xscale);
                    
                }
            }
            break;
    }
}
else
{
    image_speed = 0;
}
</string>
