                {
                    dir = "down";
                    xspeed = (lastRail.x + 8) - (x + xOffset);
                }
                else if (y + yOffset &lt;= lastRail.y + 8
                    &amp;&amp; dir == "up")
                {
                    dir = "left";
                    yspeed = (lastRail.y + 8 - yOffset) - (y);
