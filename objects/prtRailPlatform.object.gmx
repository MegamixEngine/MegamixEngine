                {
                    dir = "left";
                    yspeed = (lastRail.y + 8 - yOffset) - (y);
                }
                if (dir == "fall"
                    &amp;&amp; y + yOffset &gt;= lastRail.y + 8)
                {
                    yspeed = (lastRail.y + 8 - yOffset) - (y);
                    if (xspeed &gt; 0)
                        dir = "right";
                    else
                        dir = "left";
                }
                break;
            case 6: // bumper 
                if (lastRail != prevRail)
                    bumped = true;
                else
                    break;
                if (dir == "fall")
                {
                    x -= xspeed;
                    y -= yspeed;
                    xspeed = -xspeed;
                    yspeed = -yspeed;
                }
                else
