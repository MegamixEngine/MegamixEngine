if (entityCanStep())
{
    bumped = false;
    event_user(0);
    
    if (dir == "none")
    {
        dir = "fall";
    }
    
    if (dir != "fall")
    {
        xspeed = 0;
        yspeed = 0;
    }
    if (instance_exists(lastRail))
    {
        switch (lastRail.dir)
        {
            case 0: // horizontal 
                if (dir == "fall")
