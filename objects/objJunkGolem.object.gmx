          <argument>
            <kind>1</kind>
            <string>event_inherited();

if (entityCanStep())
{
    timer += 1;
    switch (state)
    {
        case 0: // Triggering falling 
            if (timer &gt; 40)
            {
                y = view_yview + 4;
                state = 1;
                timer = 0;
                grav = 0.25;
                ground = 0;
            }
            break;
        case 1: // Falling 
            if (y &gt;= ystart)
            {
                y = ystart;
                yspeed = 0;
                blockCollision = 1;
                ground = 1;
                state = 2;
                timer = 0;
            }
            break;
        case 2: // Throw TRAAAAAAASH 
            if (timer == 64)
            {
                instance_create(x - 16, view_yview, objThrownJunk);
                state = 3;
            }
            break;
    }
    
    if (collision_rectangle(x, y - 16, x, y, objThrownJunk, false, true)
        &amp;&amp; image_index == 0)
    {
        image_index = 1;
    }
    
    if (image_index == 2)
    {
        animTimer += 1;
        if (animTimer &gt;= 12)
        {
            image_index = 1; //0;
            animTimer = 0;
            /*timer = -45;
            state = 2;*/
        }
    }
    if (image_index == 1) &amp;&amp; (hasThrown)
    {
        animTimer += 1;
        if (animTimer &gt;= 4)
        {
            image_index = 0;
            animTimer = 0;
            timer = -45;
            state = 2;
            hasThrown = false;
        }
    }
}
else if (dead)
{
    grav = 0;
    blockCollision = 0;
    hasThrown = false;
    state = 0;
    timer = 0;
    animTimer = 0;
    image_index = 0;
}

contactDamage = 4 * visible;
</string>
