        if (stateTimer &gt;= 20 &amp;&amp; yspeed == 0)
        {
            state = 2;
            playSFX(sfxThunderWoolThunder);
            stateTimer = 0;
        }
    }
    
    if (state == 2) // shoot
    {
        // if being reflected, dont shoot thunderbolts
        if (!canDamage)
        {
            stateTimer = 1;
        }
        
        if (!(stateTimer mod 5))
