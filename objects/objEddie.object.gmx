if (entityCanStep())
{
    /*if (ycoll * sign(image_yscale) &gt; 0)
    {
        playSFX(sfxLand);
    }*/
    
    if (phase == 0)
    {
        if (ground &amp;&amp; moveTowardsPlayer)
        {
            if (!(timer mod 6))
            {
                image_index = (image_index + 1) mod 3;
            }
            
            calibrateDirection();
            xspeed = image_xscale;
        }
        else
        {
            xspeed = 0;
            if !moveTowardsPlayer
            {
                calibrateDirection();
            }
        }
        
        if (instance_exists(target))
        {
            if (abs(x - target.x) &lt;= 40)
            {
                timer = 0;
                phase = 1;
                image_index = 0;
                xspeed = 0;
            }
        }
    }
    else
    {
        switch (timer)
        {
            case 32:
                image_index = 3;
                break;
            case 40:
                image_index = 0;
                break;
            case 48:
                image_index = 4;
                break;
            case 56:
                image_index = 5;
                event_user(0);
                respawn = false;
                with (objGlobalControl)
                {
                    ds_list_add(pickups, string(room) + '/' + string(other.id));
                }
                break;
            case 72:
                image_index = 0;
                break;
            case 112:
                event_user(EV_DEATH);
                break;
        }
    }
    
    timer++;
}
else if (dead)
{
    image_index = 0;
    phase = 0;
    timer = 0;
}
</string>
