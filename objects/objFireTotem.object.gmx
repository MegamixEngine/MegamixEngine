            <string>/// Make a new collision mask
event_inherited();
if (init)
{
    init = 0;
    if (size &lt;= 0)
        size = 1;
    if (size &gt; 16)
        size = 16;
    
    var surf;
    surf = surface_create(256, 224);
    surface_set_target(surf);
    draw_clear_alpha(c_black, 0);
    draw_set_color(c_red);
    draw_rectangle(0, 0, 16, 28 + size * 8, false);
