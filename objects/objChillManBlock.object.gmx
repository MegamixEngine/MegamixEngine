        if (breakTimer &gt; 0)
            breakTimer -= 1;
        if (breakTimer == 0)
        {
            healthpoints = 0;
            event_user(EV_HURT);
            event_user(EV_DEATH);
            dead = true;
        }
    }
    if (healthpoints &gt; 1)
        image_index = 0;
    else
