                if ((faction == 3 || faction == 1) &amp;&amp; !dead &amp;&amp; ground &amp;&amp; ycoll != 0 &amp;&amp; object_index != objBusterShotCharged)
                {
                    if (place_meeting(x, y + sign(grav), other.id))
                    {
                        with (other)
                        {
                            healthpoints = max(0, healthpoints - 1);
