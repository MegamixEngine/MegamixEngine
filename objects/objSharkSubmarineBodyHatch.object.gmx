                {
                    calibrateDirection(other.target);
                }
            }
        }
        if (timer &gt;= phaseMultiple * 7) // Hatch fully closed.
        {
            hatchIndex = 0;
        }
        timer++;
        image_index = hatchIndex; // Making sure the hitbox changes when opened.
    }
    else
    {
        instance_destroy();
    }
}
