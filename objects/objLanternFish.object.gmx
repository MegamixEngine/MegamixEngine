            if (dead &amp;&amp; hasDiedBefore)
            {
                instance_destroy();
            }
        }
    
    }
    image_speed = 0.15;
    if (alarmFish &gt; 0)
    {
        alarmFish -= 1;
    }
    
    var objectCount = 0;
    
    with (objectToSpawn)
    {
        if (!dead)
        {
            objectCount++;
        }
    }
    
    if (alarmFish == 0 &amp;&amp; objectCount &lt; objectLimit)
    {
        alarmFish = 60;
        a = instance_create(x + (15 * image_xscale), y - 10, objectToSpawn);
        a.respawn = false;
        if (forceObjectFacePlayerOnSpawn) {
        a.facePlayerOnSpawn = true;
        }
    }
    if (image_index &gt;= 4)
    {
        image_index = 0;
    }
}
else
{
    image_speed = 0;
    if (killed)
    {
        if (deadTimer &lt; deadDelay)
        {
            if ((floor(deadTimer / 10) mod 2) == 0)
                image_index = deadFrame - 1;
            else
                image_index = deadFrame;
        }
        else
            image_index = deadFrame;
    }

}
