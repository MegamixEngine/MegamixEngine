          <argument>
            <kind>1</kind>
            <string>/// Enemy AI
event_inherited();

// Simple animation based enemy, it shoots when it opens and starts over when it closes

if (entityCanStep())
{
    var prevAnim = anim;
    if (anim == -1)
        anim = 0;
    if (anim == 0)
    {
        timer += 1;
        if (timer &gt; 40)
        {
            timer = 0;
            anim = 1;
        }
    }
    else if (anim == 1)
    {
        timer += 1;
        if (timer &gt; 40)
        {
            animFrame = 0;
            animSpeed = 0;
        }
        if (timer &gt; 80)
        {
            anim = 2;
            timer = 0;
        }
    }
    else if (anim == 2)
    {
        timer += 1;
        if (timer &gt; 96)
        {
            anim = 3;
            timer = 0;
        }
    }
    
    /// animation
    if (anim == 0) // blink
    {
        if (prevAnim != anim)
            animFrame = 0;
        animSpeed = 0.15;
    }
    else if (anim == 1) // blink faster
    {
        if (prevAnim != anim)
            animSpeed = 0.3;
    }
    else if (anim == 2) // open
    {
        animSpeed = 0;
        if (prevAnim != anim)
        {
            animFrame = 2;
            event_user(0); // Shoot
        }
    }
    else if (anim == 3) // close
    {
        animSpeed = 0.25;
    }
    
    animFrame += animSpeed;
    if ((anim == 1 || anim == 0) &amp;&amp; floor(animFrame) &gt; 1)
        animFrame = 0;
    
    image_index = floor(animFrame);
}
</string>
