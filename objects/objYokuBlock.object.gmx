spike = false; // don't set creation code to make yoku spikes, use the objYokuSpike object

timer = 0;
phase = 0; // 0 = waiting to appear; 1 = active; 2 = waiting to reappear after disappearing; 3 = inactive;
mySolid = -12;

respawnRange = -1;
despawnRange = -1;

alarm[0] = 1;
