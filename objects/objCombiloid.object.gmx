    {
        case 0: // wait for refill 
            if (instance_exists(partner))
            {
                xMax = partner.xMax;
                xMin = partner.xMin;
                with (partner)
                {
                    phase = 3;
                    if (x &gt;= other.x - 1 &amp;&amp; x &lt;= other.x + 1)
                    {
                        xspeed = 0;
                        other.phase = 1;
                        playSFX(sfxAbsorb);
                    }
                    else if (x &gt; other.x)
                    {
                        xspeed = -1;
                    }
                    else if (x &lt; other.x)
