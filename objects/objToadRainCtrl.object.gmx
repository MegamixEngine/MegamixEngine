            tile_layer_hide(mylayer);
            tile_layer_show(mylayer + 1);
            tile_layer_hide(mylayer + 2);
        }
        else if (img == 3)
        {
            tile_layer_hide(mylayer);
            tile_layer_hide(mylayer + 1);
            tile_layer_show(mylayer + 2);
        }
        img += 1;
        if (img == 4)
        {
            img -= 3;
        }
    }
}
