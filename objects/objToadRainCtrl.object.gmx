                            }
                        }
                    }
                }
            }
            else
            {
                shiftObject(other.blowSpeed, 0, 1);
            }
        }
    }
    
    imgalarm += 1;
    if (imgalarm &gt;= 8)
    {
        imgalarm = 0;
