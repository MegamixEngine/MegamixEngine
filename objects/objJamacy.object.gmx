if (entityCanStep())
{
    xSpeedTurnaround();
    
    // Get from ladder on ground
    if (climbing)
    {
        if (ycoll * image_yscale &gt; 0)
        {
            climbing = 0;
            sprite_index = sprJamacy;
        }
    }
    
    // Dismount Ladder
    if (climbing)
