        <arguments>
          <argument>
            <kind>1</kind>
            <string>event_inherited();

if (entityCanStep())
{
    timer++;
    switch (phase)
    {
        case 0: // Move 
            xspeed = 1.25 * image_xscale;
            if (timer &gt;= 40)
            {
                xspeed = 0;
                image_index = 1; // pop out
                timer = 0;
                phase = 1;
            }
            break;
        case 1: // Shoot the 8-way shot. 
            if (timer &gt;= 15)
            {
                playSFX(sfxEnemyShootClassic);
                var t;
                for (i = 0; i &lt; bullets; i++)
                {
                    t = instance_create(x, y, obj);
                    t.sprite_index = objectSprite;
                    t.contactDamage = objectDamage;
                    t.spd = objectSpeed;
                    t.dir = (180 * (image_xscale &lt; 0)) + (i * (360 / bullets));
                }
                timer = 0;
                phase = 2;
            }
            break;
        case 2: // Reset to moving 
            if (timer &gt;= 20)
            {
                image_index = 0;
                timer = -20;
                phase = 0;
            }
            break;
    }
}
else if (dead)
{
    timer = 0;
    phase = 0;
    image_index = 0;
}
</string>
