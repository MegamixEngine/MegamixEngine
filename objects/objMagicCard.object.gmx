                {
                    pickup.depth = depth - 1;
                    with (object_index)
                    {
                        if (id != other.id)
                        {
                            if (pickup == other.pickup)
                            {
                                other.pickup = 0;
                            }
                        }
                    }
                }
                else
                {
                    pickup = 0;
                }
            }
        }
        if (pickup) // Control collected pickup
        {
            if (instance_exists(pickup))
            {
                var pickx = spriteGetXCenterObject(pickup) - pickup.x;
                var picky = spriteGetYCenterObject(pickup) - pickup.y;
                pickup.x = x - pickx + 2 * image_xscale;
                pickup.xspeed = 0;
                pickup.y = y - picky + 2;
                pickup.yspeed = 0;
            }
            else
            {
                pickup = 0;
            }
        }
    }
