        timeSlow--;
        if (timeSlow == 0)
        {
            if (invertColor)
                invertColor = false;
            else
                invertColor = true;
            timeSlow = 2;
        }
    }
    
    if (moveTimer == 0)
    {
        invertColor = false;
        playSFX(sfxTimeStopper);
        timeSlow = 15;
    }
    
    if (moveTimer &lt; 0)
    {
        if instance_exists(target)
        {
            with (target)
            {
                // Create toxin object
                if (!instance_exists(objToxinHandler))
                {
                    with (instance_create(x,y,objToxinHandler))
                    {
                        parent = other.id;
                        inkStatus = other.inked;
                        gravOriginal = other.gravfactor;
                    }
                }
            }
        }
        
        // Create flash
        timeSlow--;
        if (timeSlow == 0)
        {
            if (!instance_exists(objFooleyFlash))
            {
                instance_create(x, y, objFooleyFlash);
            }
            else
            {
                with (objFooleyFlash)
                {
                    instance_destroy();
                }
            }
            timeSlow = 20;
        }
    }
    // Shoot
    shootTimer--;
    if (shootTimer == 0)
    {
        instance_create(x, y, objFooleyShot);
        playSFX(sfxEnemyShootClassic);
        shootTimer = shootTimerMax;
    }
    imgIndex += imgSpd; // Animation
    if (imgIndex &gt;= 8)
        imgIndex = 0;
}
image_index = imgIndex div 1 + (8 * invertColor);
