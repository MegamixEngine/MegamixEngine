            <string>/// A pseudo miniboss that will shoot homing missiles at megaman.
event_inherited();

healthpointsStart = 16;
healthpoints = healthpointsStart;
contactDamage = 4;
lockTransition = false;

category = "bulky, nature";

doesIntro = false;
rescursiveExplosion = false;

// Enemy specific code
phase = 0;
