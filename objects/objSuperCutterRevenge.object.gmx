            var theta = pi * 2 * roundTo(phaseTimer / period, 1 / 16);
            x = xstart + radius * cos(theta) * dir;
            y = ystart - radius * sin(theta);
            animTimer = (animTimer + 1) mod 24;
            var animTable = makeArray(1, 0, 2, 0)
                ;
            image_index = animTable[animTimer div 6];
    }
}
