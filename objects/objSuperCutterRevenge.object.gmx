            // begin circling:
            if (p &gt;= 0.85)
            {
                phase = 2;
                animTimer = 0;
                phaseTimer = 0;
            }
            break;
        case 2: // circle
        // motion is discontinuous and defies all sense. 
            xspeed = 0;
            yspeed = 0;
