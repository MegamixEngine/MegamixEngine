        directionStore = image_xscale;
        moveTimer = 0;
    }
    
    // if it walks off an edge, turn round.
    if (!ground)
    {
        xspeed = -xspeed;
        x += xspeed * 2;
        image_xscale = -image_xscale;
        directionStore = image_xscale;
        moveTimer = 0;
    }
    
    // spawn head
    if (spawnHead)
