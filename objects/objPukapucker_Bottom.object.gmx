        inst = instance_create(x, y - 32, objPukapucker_Top);
        with (inst)
        {
            // attached head default values.
            respawn = false;
            bodyInstanceStore = headID;
        }
        spawnHead = false;
    }
    
    // Animation
    if (!animBack)
    {
        imgIndex += imgSpd;
        if (imgIndex &gt;= 3)
        {
            imgIndex = 2;
            animBack = true;
        }
    }
    else
    {
        imgIndex -= imgSpd;
        if (imgIndex &lt; 0)
        {
            imgIndex = 1;
            animBack = false;
        }
    }
