        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (!visible)
    exit;

switch (emoteType) {
    case 0: // ?
    case 1: // !
        if (substate == 0) {
            y -= 3;
            
            if (y &lt;= ystart - 4) {
                y = ystart;
                substate++;
            }
        }
        
        imgIndex = emoteType;
        break;
    
    case 2: // ...
        imgIndex = min(2 + (timer div 24), 4);
        break;
    
    case 3: // X
        if (timer == 0)
            playSFX(sfxExclamationPoint);
        
        if (substate == 0) {
            y += yspeed;
            yspeed = min(yspeed + grav, 7);
            
            if (yspeed &gt;= 0 &amp;&amp; y &gt;= ystart) {
                y = ystart;
                substate++;
            }
        }
        
        imgIndex = 8 + ((timer mod 16) &lt; 8);
        break;
}

timer++;
if (--lifeDuration &lt;= 0)
    instance_destroy();
</string>
