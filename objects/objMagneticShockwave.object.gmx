    {
        instance_destroy();
    }
    
    if (number &lt; 4)
    {
        if (spawnTimer) // spreading across
        {
            spawnTimer--;
            if (!spawnTimer &amp;&amp; insideView())
            {
                i = instance_create(x + (28 * sign(image_xscale)), y, objMagneticShockwave);
                i.number = number + 1;
                i.image_xscale = image_xscale;
                i.image_yscale = image_yscale;
                i.faction = faction;
                i.contactDamage = contactDamage;
                i.playerID = playerID;
            }
        }
    }
    
    if (timer == 0)
    {
        // screenShake(2, 1, 1);
        if (!positionCollision(x - 8, y + image_yscale) || !positionCollision(x + 8, y + image_yscale))
        {
            i = instance_create(x, y, objSingleLoopEffect);
            i.sprite_index = explosionSprite;
            i.image_index = 1;
            i.image_speed = 0.15;
            i.useShader = (global.multiplayerColors &amp;&amp; global.playerCount &gt; 1);
            
            i.replaceColor[0] = $ADCFFF;
            i.replaceColor[1] = $FFFFFF;
            
            i.newColor[0] = global.multiplayerPalette[playerID,global.weapon[playerID]];
            i.newColor[1] = merge_color(global.multiplayerPalette[playerID,global.weapon[playerID]],c_white,0.85);
        }
    }
    
    timer++;
    if (!(timer mod 3))
    {
        i = instance_create(irandom_range(x - 7, x + 7), y, objSingleLoopEffect);
        i.sprite_index = particleSprite;
        i.image_speed = 0.4;
        i.vspeed = (-irandom(25) / 10 * (1 - ((timer mod 6) == 0) * 1.33)) * image_yscale;
        i.useShader = (global.multiplayerColors &amp;&amp; global.playerCount &gt; 1);
        i.replaceColor[0] = multPal[0];
        i.newColor[0] = global.multiplayerPalette[playerID,global.weapon[playerID]];
        i.replaceColor[1] = multPal[2];
        i.newColor[1] = merge_color(global.multiplayerPalette[playerID,global.weapon[playerID]],c_white,0.75);
    }
}

