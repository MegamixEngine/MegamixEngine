            <string>event_inherited();

if (entityCanStep())
{
    if ((!noSpeedException &amp;&amp; xspeed == 0) || ground)
    {
        instance_create(x, y, objExplosion);
        playSFX(getGenericSFX(SFX_ENEMYHIT));
        instance_destroy();
    }
    
    image_speed = 0.3;
}
else
{
    image_speed = 0;
}
