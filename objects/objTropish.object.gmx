                }
            }
            break;
        case 2: // Speed up 
            if (abs(xspeed) &lt; 4)
            {
                xspeed += .25 * image_xscale;
            }
            
            // bob up and down
            if (timer mod 10 == 0)
            {
                y += 1.5 * flip;
                flip = -flip;
            }
            
            // Animation
            if (timer mod 4 == 0)
            {
                imgOffset = !imgOffset;
            }
            break;
    }
}
else if (dead)
{
    timer = 0;
    phase = 1;
    imgOffset = 0;
    flip = 1;
}

visible = (phase &gt; 1);
canHit = visible;
