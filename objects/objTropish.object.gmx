            if (instance_exists(target))
            {
                if (collision_line(x - 256, y + 8, x + 256, y + 8, target, false, true))
                {
                    if (image_xscale == 1)
                    {
                        x = view_xview;
                    }
                    else
                    {
                        x = view_xview + 256;
                    }
                    
                    xspeed = 1.5 * image_xscale;
                    
