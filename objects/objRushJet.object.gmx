                    if (checkSolid(0, other.yspeed*2))
                    {
                        other.yspeed = 0;
                    }
                }
                
                isSolid *= -1;
            }
            else
            {
                // not on player
                event_perform(ev_destroy, 0);
                yspeed = 0;
                spd = maxSpd;
            }
        }
    }
    
