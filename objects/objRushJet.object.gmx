                if (parent.ground &amp;&amp; !place_meeting(x, y, parent))
                {
                    canJet = false;
                    timer = -1;
                }
            }
        }
        else // Flying
        {
            xspeed = spd * image_xscale;
            
            // Take away ammo
            decreaseAmmoTimer += 1;
            if (decreaseAmmoTimer &gt;= decreaseAmmoTimerMax)
